{
  "name": "reverse-geocode-lite",
  "version": "1.0.1",
  "description": "A lightweight TypeScript-based reverse geocoding utility using OpenStreetMap's Nominatim API.",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "scripts": {
    "clean": "rm -rf build",
    "build": "npm run clean && tsc",
    "dev": "tsc --watch",
    "lint": "eslint src --ext .ts,.tsx",
    "test": "jest"
  },
  "files": [
    "build/*.js",
    "build/*.js.map",
    "build/*.d.ts",
    "CONTRIBUTING.md",
    "README.md"
  ],
  "keywords": [
    "reverse",
    "geocode",
    "geocoding",
    "openstreetmap",
    "location",
    "coordinates",
    "nominatim"
  ],
  "author": "Jonahgeek",
  "repository": {
    "type": "git",
    "url": "https://github.com/jonahgeek/reverse-geocode-lite.git"
  },
  "license": "MIT",
  "dependencies": {
    "node-fetch": "^3.3.2"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "jest": "^29.7.0",
    "ts-jest": "^29.3.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.2"
  },
  "type": "module",
  "engines": {
    "node": ">=16"
  }
}
